	var WL = '';
	var RL = [];
	var RDLC = 0;
	var CM = 0;
	var C = [];
	var BD = [];
	var BE = [];
	var PD = [];
	var PE = [];
	var T1 = [];
	var T2 = [];
	var TT = '';
	var TIP = [];
	var TIPC = 0;

	do {
		var D = parseInt(prompt('‚Üí  ùë´ùë∞ùë≠ùë∞ùë™ùëºùë≥ùë´ùë®ùë´ùë¨ùë∫:     ·ï¶( Õ°¬∞ Õú ñ Õ°¬∞)·ï§' + '\n- Digite 1 para jogar no modo ùíáùíÇùíÑùíäùíç;' + '\n- Digite 2 para jogar no n√≠vel ùíéùíÜùíÖùíäùíê;' + '\n- Digite 3 para jogar no modo ùíÖùíäùíáùíäùíÑùíäùíç.'));
	} while (D != 1 && D != 2 && D != 3);
	switch (D) {
		case 1: 
			var T = ['_', '_', '_', '_', '_', '_'];
			TT = T.join(' ');
			var F = [[['  '],['_'],['_'],['_'],['_'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],['|'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],[' '],[C],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[BE],[T1],[BD],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],[T2],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[PE],[' '],[PD],['  '],['  ']],[['_'],['|'],['_'],['  '],['  '],['  '],['  '],['  ']],[['|'],['   '],['|'],[' '],[TT]],[['|'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['|']]];
			break;
		case 2:
			var T = ['_', '_', '_', '_', '_', '_', '_'];
			TT = T.join(' ');
			var F = [[['  '],['_'],['_'],['_'],['_'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],['|'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],[' '],[C],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[BE],[T1],[BD],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],[T2],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[PE],[' '],[PD],['  '],['  ']],[['_'],['|'],['_'],['  '],['  '],['  '],['  '],['  ']],[['|'],['   '],['|'],[' '],[TT]],[['|'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['|']]];
			break;
		case 3:
			var T = ['_', '_', '_', '_', '_', '_', '_', '_'];
			TT = T.join(' ');
			var F = [[['  '],['_'],['_'],['_'],['_'],['  '],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],['|'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],[' '],[C],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[BE],[T1],[BD],['  '],['  ']],[['  '],['|'],['  '],['   '],[PE],[' '],[PD],['  '],['  ']],[['_'],['|'],['_'],['  '],['  '],['  '],['  '],['  '],['  ']],[['|'],['  '],['|'],[' '],[TT]],[['|'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['|']]];
			break;
	}
	var WS = SortWord (W, D);
	if (D == 1 || D == 2) {	
		do {
			TT = T.join(' ');
			var G = prompt('LETRAS ERRADAS: ' + WL + '\n' + RenewT(F)).toUpperCase();
			if (G === '*' && RDLC != 2) {
				RDLC++;
				RandomLetters (RL);
			} 
			if (G === '%') {
				TIPC++;
				Tips (D, W, WS, TIPC);
			}
			if (G.length > 1 && G.length == WS.length) {
				if (G == WS.join('')) {
					alert('Voc√™ acertou, a palavra era ' + WS.join('')  + '!'+ '  „ÉΩ(‚åí‚àá‚åí)Ôæâ');
					break;
				} else {
					alert('Voc√™ perdeu, a palavra era ' + WS.join('') + '!' + '   o(‚ï•Ôπè‚ï•)o');
					break;
				}
			} 
			RightOrWrong (WS, G);
			HangmanMembers(F);	
		} while (T.join() != WS.join() && CM != 7);
		if (T.join('') == WS.join('')) {
			alert('Voc√™ acertou, a palavra era ' + WS.join('')  + '!'+ '  „ÉΩ(‚åí‚àá‚åí)Ôæâ');
		}
		if (CM == 7) {
			alert('Voc√™ perdeu, a palavra era ' + WS.join('') + '!' + '   o(‚ï•Ôπè‚ï•)o');
		} 
	} else if (D == 3) {
		do {
			TT = T.join(' ');
			var G = prompt('LETRAS ERRADAS: ' + WL + '\n' + RenewT(F)).toUpperCase();
			if (G === '*' && RDLC != 2) {
				RDLC++;
				RandomLetters (RL);
			} 
			if (G === '%') {
				TIPC++;
				Tips (D, W, WS, TIPC);
			}
			if (G.length > 1 && G.length == WS.length) {
				if (G == WS.join('')) {
					alert('Voc√™ acertou, a palavra era ' + WS.join('')  + '!'+ '  „ÉΩ(‚åí‚àá‚åí)Ôæâ');
					break;
				} else {
					alert('Voc√™ perdeu, a palavra era ' + WS.join('') + '!' + '   o(‚ï•Ôπè‚ï•)o');
					break;
				}
			} 
			RightOrWrong (WS, G);
			HangmanMembers(F);	
		} while (T.join() != WS.join() && CM != 6);
		if (T.join('') == WS.join('')) {
			alert('Voc√™ acertou, a palavra era ' + WS.join('')  + '!'+ '  „ÉΩ(‚åí‚àá‚åí)Ôæâ');
		}
		if (CM == 6) {
			alert('Voc√™ perdeu, a palavra era ' + WS.join('') + '!' + '   o(‚ï•Ôπè‚ï•)o');
		} 
	}
	do {
		var PlayAgain = prompt('‚ûÆ Deseja jogar novamente? (/‚Ä¢‡∏¥_‚Ä¢‡∏¥)/" + "\n(Responda com S ou N)').toUpperCase();
	} while (PlayAgain != 'S' && PlayAgain != 'N');
} while (PlayAgain != 'N');
alert("FIM DE JOGO! „ÉΩ(‚äôÔºê‚äô)„Éé" + "\n" + "\nObrigada por jogar!" + "\n" + "\nCRIADO POR: Antonella Cuello  („Å§‚ñÄ¬Ø‚ñÄ)„Å§");
