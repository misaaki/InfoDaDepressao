//JOGO DA FORCA - PROJETO TCA
function SortWord (W, D) {
	var S = Math.floor(Math.random() * 100);
	var WSS = '';
	switch (D) {
		case 1:
			WSS = W[0][S];
			break;
		case 2: 
			WSS = W[1][S];
			break;
		case 3:
			WSS = W[2][S];
			break;
	}
	return WSS.split('');
}
function RightOrWrong (WS, G) {
	for (var CG = 0; CG < WS.length; CG++) {
		if (G == WS[CG]) {
			T.splice(CG, 1, G);
			RL.push(G);
		}
		if (WS.indexOf(G) == -1 && WL.indexOf(G) == -1 && G.length == 1 && G != '*' && G != '%') {
			WL += G + ', ';
			CM++;
		}
	}
}

function HangmanMembers (G) {
	switch (D) {
		case 1: case 2: 
			switch (CM) {
				case 1:
					C = ['O'];
					break;
				case 2: 
					T1 = ['|'];
					break;
				case 3:
					BE = ['/'];
					break;
				case 4:
					BD = ['\\'];
					break;
				case 5:
					T2 = ['|'];
					break;
				case 6:
					PE = ['/'];
					break;
				case 7:
					PD = ['\\'];
					break;
			}
			break;
		case 3:
			switch (CM) {
				case 1:
					C = ['O'];
					break;
				case 2: 
					T1 = ['|'];
					break;
				case 3:
					BE = ['/'];
					break;
				case 4:
					BD = ['\\'];
					break;
				case 5:
					PE = ['/'];
					break;
				case 6:
					PD = ['\\'];
					break;
			}
			break;
	}
}
function RandomLetters (RL) {
	do {
		var S = Math.floor(Math.random() * WS.length);
	} while (RL.indexOf(WS[S]) != -1);
	return G = WS[S];
}
function Tips (D, W, WS, TIPC) {
	switch (D) {
		case 1:
			if (W[0].indexOf(WS.join('')) <= 10) {
				TIP = ['√â um personagem masculino', ' de um anime', ' shounen.'];
			}
			if (W[0].indexOf(WS.join('')) > 10 && W[0].indexOf(WS.join('')) <= 20) {
				TIP = ['√â um objeto', ' que voc√™ costuma ver diariamente', ' na escola.'];
			}
			if (W[0].indexOf(WS.join('')) > 20 && W[0].indexOf(WS.join('')) <= 30) {
				TIP = ['√â um animal', ' pequeno', ' que voa.'];
			}
			if (W[0].indexOf(WS.join('')) > 30 && W[0].indexOf(WS.join('')) <= 40) {
				TIP = ['Tem rela√ß√£o com o gen√™ro musical kpop', ', mais especificamente um idol', ' de um boygroup.'];
			}
			if (W[0].indexOf(WS.join('')) > 40 && W[0].indexOf(WS.join('')) <= 50) {
				TIP = ['Tem rela√ß√£o com um personagem', ' de um jogo', ' chamado League Of Legends.'];
			}
			if (W[0].indexOf(WS.join('')) > 50 && W[0].indexOf(WS.join('')) <= 60) {
				TIP = ['Personagem', ' de uma anima√ß√£o', ' infantil'];
			}
			if (W[0].indexOf(WS.join('')) > 60 && W[0].indexOf(WS.join('')) <= 70) {
				TIP = ['Tem rela√ß√£o com moda', ', faz parte do vestu√°rio', ' e pode ser um acess√≥rio ou roupa.'];
			}
			if (W[0].indexOf(WS.join('')) > 70 && W[0].indexOf(WS.join('')) <= 80) {
				TIP = ['Tem rela√ß√£o com o IF', ', √© um colega', ' de inform√°tica.'];
			}
			if (W[0].indexOf(WS.join('')) > 80 && W[0].indexOf(WS.join('')) <= 90) {
				TIP = ['N√£o √© um objeto ou alimento', ', √© um nome pr√≥prio', ' feminino.'];
			}
			if (W[0].indexOf(WS.join('')) > 90 && W[0].indexOf(WS.join('')) <= 100) {
					TIP = ['√â um funcion√°rio', ' do IF', ', mais especificamente um professor.'];
			}
			switch (TIPC) {
				case 1:
					alert(TIP[0] + '...');
					break;
				case 2:
					alert(TIP[0] + TIP [1] + '...');
					break;
				case 3:
					alert(TIP.join(''));
			}
			break;
		case 2:
			TIP = ['Neste n√≠vel n√£o existem dicas. :('];
			alert(TIP.join(''));
			break;
		case 3:
			if (W[2].indexOf(WS.join('')) <= 10) {
				TIP = ['√â um animal', ' que geralmente n√£o √© mantido dentro de casa, ou pelo menos n√£o deveria.'];
			}
			if (W[2].indexOf(WS.join('')) > 10 && W[0].indexOf(WS.join('')) <= 20) {
				TIP = ['√â um alimento', ', em especif√≠co, uma fruta.'];
			}
			if (W[2].indexOf(WS.join('')) > 20 && W[0].indexOf(WS.join('')) <= 30) {
				TIP = ['Faz parte da categoria alimentos', ' e √© doce.'];
			}
			if (W[2].indexOf(WS.join('')) > 30 && W[0].indexOf(WS.join('')) <= 40) {
				TIP = ['√â um nome pr√≥prio', ' masculino.'];
			}
			if (W[2].indexOf(WS.join('')) > 40 && W[0].indexOf(WS.join('')) <= 50) {
				TIP = ['√â um objeto', ' que voc√™ encontra em casa.'];
			}
			if (W[2].indexOf(WS.join('')) > 50 && W[0].indexOf(WS.join('')) <= 60) {
				TIP = ['N√£o √© algo f√≠sico (coisa).', ' √â um verbo.'];
			}
			if (W[2].indexOf(WS.join('')) > 60 && W[0].indexOf(WS.join('')) <= 70) {
				TIP = ['N√£o √© um objeto, nem animal, nem alimento.', ' √â um adjetivo.'];
			}
			if (W[2].indexOf(WS.join('')) > 70 && W[0].indexOf(WS.join('')) <= 80) {
				TIP = ['√â algo que remete √† crian√ßa', ', algo com que possa brincar.'];
			}
			if (W[2].indexOf(WS.join('')) > 80 && W[0].indexOf(WS.join('')) <= 90) {
				TIP = ['√â um personagem', ' do League of Legends.'];
			}
			if (W[2].indexOf(WS.join('')) > 90 && W[0].indexOf(WS.join('')) <= 100) {
				TIP = ['√â um animal marinho', ', mais especificamente um peixe.'];
			}
			switch (TIPC) {
				case 1:
					alert(TIP[0] + '...');
					break;
				case 2:
					alert(TIP.join(''));
					break;
			}
			break;
	}
}
function RenewT (F) {
	switch (D) {
	case 1: 
		F = [[['  '],['_'],['_'],['_'],['_'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],['|'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],[' '],[C],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[BE],[T1],[BD],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],[T2],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[PE],[' '],[PD],['  '],['  ']],[['_'],['|'],['_'],['  '],['  '],['  '],['  '],['  ']],[['|'],['   '],['|'],[' '],[TT]],[['|'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['|']]];
		break;
	case 2:
		F = [[['  '],['_'],['_'],['_'],['_'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],['|'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],[' '],[C],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[BE],[T1],[BD],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],[T2],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[PE],[' '],[PD],['  '],['  ']],[['_'],['|'],['_'],['  '],['  '],['  '],['  '],['  ']],[['|'],['   '],['|'],[' '],[TT]],[['|'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['|']]];		break;
		break;
	case 3:
		F = [[['  '],['_'],['_'],['_'],['_'],['  '],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],['  '],['|'],['  '],['  '],['  ']],[['  '],['|'],['  '],['  '],[' '],[C],['  '],['  '],['  ']],[['  '],['|'],['  '],['   '],[BE],[T1],[BD],['  '],['  ']],[['  '],['|'],['  '],['   '],[PE],[' '],[PD],['  '],['  ']],[['_'],['|'],['_'],['  '],['  '],['  '],['  '],['  '],['  ']],[['|'],['  '],['|'],[' '],[TT]],[['|'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['_'],['|']]];
		break;
	}
	F = F.join('\n');
	F = F.replace(/,/gi, '');
	return F;
}
do {
	do {
		var menu = prompt('ùë∫ùíÜùíãùíÇ ùíÉùíÜùíé-ùíóùíäùíèùíÖùíê ùíÇ ùíéùíäùíèùíâùíÇ ùíáùíêùíìùíÑùíÇ!   (‚ó°‚Äø‚ó°‚úø)' + '\n‚ñ∏ Digite 1 para ver as regras;' + '\n‚ñ∏ Digite 2 para ver os diferenciais/extras;' + '\n‚ñ∏ Aperte enter para jogar.');
		switch (menu) {
			case '1':
				alert('Como o jogo funciona?   („ÄÇ‚åí‚àá‚åí)„ÄÇ' + '\n‚ñ∏ O jogo da forca √© um jogo em que o jogador tem que acertar qual √© a palavra proposta, tendo como dica inicial o n√∫mero de letras dessa palavra;' + '\n‚ñ∏ A cada tentativa falha ir√° ser adicionado um membro ao corpo do enforcado, iniciando pela cabe√ßa, tronco, bra√ßos e terminando nas pernas;' + '\n‚ñ∏ O jogo termina ou com o acerto da palavra ou com o t√©rmino do preenchimento das partes corp√≥reas do enforcado, assim resultando no que chamamos de ‚Äúforca‚Äù.');
				break;
			case '2':
				alert('E os extras?   „ÄÇ(‚åí‚àá‚åí„ÄÇ)' + '\n‚ñπ O jogo √© subdividido em n√≠veis:' + '\n   1. F√ÅCIL - Contendo palavras com 6 letras e uma tentativa a mais devido ao tamanho da forca nesta dificuldade;' + '\n   2. M√âDIO - Contendo palavras de 7 letras e o mesmo n√∫mero de tentativas no modo f√°cil;' + '\n   3. DIF√çCIL - Contendo palavras de 8 letras e uma tentativa a menos, devido ao tamanho da forca.'  + '\n‚ñπ Para cada palavra no n√≠vel f√°cil ir√° ter 3 dicas. J√° no n√≠vel dif√≠cil, ir√£o ter apenas 2. Tendo em vista que no n√≠vel m√©dio tem-se a mesma quantidade de tentativas, essa funcionalidade n√£o se aplica √† esse n√≠vel. Para utilizar este comando insere-se "%".' + '\n‚ñπ Caso o jogador esteja sem ideias de letras pode-se inserir o caracter "*" que uma letra ir√° ser substitu√≠da no tracejado de maneira aleat√≥ria, por√©m este comando s√≥ poder√° ser utilizado 2 vezes;');
				break;
			case '':
				break;
		}
	} while (menu != '');
